{
  "name": "f1tenth",
  "service": "dev",
  "dockerComposeFile": "../docker/compose.dev.yml",
  "workspaceFolder": "/f1tenth",
  "initializeCommand": "docker pull ualbertaformula/f1tenth:latest && docker pull ualbertaformula/f1tenth-sim:latest",
  "shutdownAction": "stopCompose",
  "features": {
    "git": "os-provided",
    "sshd": "latest"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "twxs.cmake",
        "eamodio.gitlens",
        "ms-python.python",
        "ms-python.black-formatter",
        "ms-python.flake8",
        "ms-python.isort",
        "ms-python.mypy-type-checker@prerelease",
        "ms-python.vscode-pylance",
        "ms-iot.vscode-ros",
        "njpwerner.autodocstring"
      ]
    }
  }
}
