{
  "name": "f1tenth",
  "service": "dev",
  "dockerComposeFile": "../docker/compose.dev.yml",
  "workspaceFolder": "/f1tenth",
  "initializeCommand":
      "docker pull ualbertaformula/f1tenth:latest && docker pull ualbertaformula/f1tenth-sim:latest",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "twxs.cmake",
        "eamodio.gitlens",
        "ms-python.python",
        "ms-python.black-formatter",
        "ms-python.vscode-pylance",
        "xaver.clang-format",
        "ms-python.isort",
        "matangover.mypy",
        "ms-iot.vscode-ros"
      ]
    }
  }
}
